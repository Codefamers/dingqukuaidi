<?xml version="1.0" encoding="utf-8"?>
<inset
  xmlns:android="http://schemas.android.com/apk/res/android">
    <selector>
        <item android:state_pressed="true">
            <shape>
                <gradient android:startColor="#bcbcbc" android:endColor="#bcbcbc" android:angle="0.0" />
                <corners android:radius="5dp" />
                <stroke android:width="0dp" android:color="#bcbcbc" />
            </shape>
        </item>
        <item android:state_enabled="false">
            <shape>
                <gradient android:startColor="#e5e5e5" android:endColor="#e5e5e5" android:angle="0.0" />
                <corners android:radius="5dp" />
                <stroke android:width="0dp" android:color="#e5e5e5" />
            </shape>
        </item>
        <item>
            <shape>
                <gradient android:startColor="#e5e5e5" android:endColor="#e5e5e5" android:angle="0.0" />
                <corners android:radius="5dp" />
                <stroke android:width="0dp" android:color="#e5e5e5" />
            </shape>
        </item>
    </selector>
</inset>